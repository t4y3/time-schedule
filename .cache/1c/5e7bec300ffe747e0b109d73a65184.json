...this.data];\n  }\n\n  add(data) {\n    this.data.push(data);\n    localStorage.setItem(KEY, JSON.stringify(this.data.sort((a, b) => a.time - b.time)));\n    this.emit('change');\n  }\n\n  remove(idx) {\n    this.data.splice(idx, 1);\n    localStorage.setItem(KEY, JSON.stringify(this.data.sort((a, b) => a.time - b.time)));\n    this.emit('change');\n  }\n\n  update(idx, item) {\n    this.data[idx] = Object.assign(this.data[idx], item);\n    localStorage.setItem(KEY, JSON.stringify(this.data.sort((a, b) => a.time - b.time)));\n    this.emit('change');\n  }\n\n  on(name, fn) {\n    if (!this.handler[name]) {\n      this.handler[name] = [];\n    }\n    this.handler[name].push(fn);\n  }\n\n  emit(name) {\n    this.handler[name].forEach(fn => {\n      fn();\n    });\n  }\n}\n\nexport default new Store();\n"},"lineCount":null}},"error":null,"hash":"d23fce505c472c80fb84ab973a8430ee","cacheData":{"env":{}}}