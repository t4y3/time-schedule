is.list = this.store.get();\n    this.selectedNo = -1;\n    this.handleItemClick = this.handleItemClick.bind(this);\n    this.handleAddClick = this.handleAddClick.bind(this);\n    this.handleLayerClick = this.handleLayerClick.bind(this);\n    this.handleSaveClick = this.handleSaveClick.bind(this);\n  }\n\n  __template() {\n    return html`\n      <div class=\"container\">\n        <div class=\"body\">\n          <div class=\"list\">\n            ${\n              repeat(\n                this.list,\n                item => {\n                  const date = new Date(item.time);\n                  return html`\n                    <div class=\"list__item item\" @click=\"${this.handleItemClick}\">\n                      <div class=\"item__time\">${date.getHours()}:${date.getMinutes()}</div>\n                      <div class=\"item__text\">${item.text}</div>\n                    </div>\n                  `\n                }\n              )\n          }\n          </div>\n          <div class=\"add\" @click=\"${this.handleAddClick}\">追加</div>\n        </div>\n        <div class=\"tail\"></div>\n        <div class=\"${\n          classMap({\n            sheet: true,\n            'sheet--active': this.selectedNo > -1\n          })}\"\n          @click=\"${this.handleLayerClick}\"\n        >\n          <div class=\"sheet__container\">\n            <div class=\"wrap\">\n              <div class=\"hour\">\n                ${\n                  repeat(\n                    Array.from({length: 24}, (v, i) => i),\n                    item => html`\n                      <div class=\"hour__item\">${item}</div>                    \n                    `\n                  )\n                }\n                \n              </div>\n              <div class=\"time\">\n                ${\n                  repeat(\n                    Array.from({length: 60}, (v, i) => i),\n                    item => html`\n                      <div class=\"time__item\">${item}</div>                    \n                    `\n                  )\n                }  \n              </div>\n            </div>\n            <div class=\"save\" @click=\"${this.handleSaveClick}\">save</div>\n          </div>\n        </div>\n`\n  }\n\n  __render() {\n    render(this.__template(), document.getElementById('app'));\n  }\n\n  handleItemClick() {\n    this.selectedNo = 1;\n    this.__render();\n  }\n\n  handleAddClick() {\n    this.selectedNo = 1;\n    this.__render();\n  }\n\n  handleSaveClick() {\n    this.store.add({\n      time: Date.now(),\n      text: 'clickkkkkk'\n    });\n    this.selectedNo = -1;\n    this.__render();\n  }\n\n  handleLayerClick(e) {\n    if (e.target.classList.contains('sheet') ) {\n      this.selectedNo = -1;\n      this.__render();\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"eb6e2a599c0d0227d6769c197b088012","cacheData":{"env":{}}}